{
  "name": "quis",
  "version": "1.2.0",
  "description": "A simple DSL for data sorting and filtering",
  "main": "build/quis-ts.cjs",
  "module": "build/quis-ts.cjs",
  "browser": "build/quis.min.js",
  "types": "build/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "import": "./build/quis-ts.cjs",
      "require": "./build/quis-ts.cjs",
      "browser": "./build/quis.min.js"
    },
    "./types": {
      "types": "./build/types/index.d.ts"
    }
  },
  "files": [
    "build",
    "src",
    "test",
    "scripts",
    "docs"
  ],
  "keywords": [
    "DSL",
    "data",
    "sorting",
    "filtering"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/videlais/quis"
  },
  "scripts": {
    "test": "jest",
    "test:node": "jest --testMatch='**/test/node/**/*.test.{js,ts}'",
    "test:web": "jest --testMatch='**/test/web/**/*.test.{js,ts}' --testEnvironment=jsdom",
    "test:all": "npm run test:node && npm run test:web",
    "compile": "peggy -o ./build/quis.cjs ./src/dsl.pegjs",
    "build:types": "tsc --emitDeclarationOnly",
    "build:webpack": "webpack --mode=production",
    "build": "npm run build:types && npm run build:webpack",
    "build:dev": "webpack --mode=development",
    "clean": "rimraf build/quis.js build/quis-ts.cjs build/quis.min.js build/types",
    "lint": "eslint ./src ./test --fix",
    "lint:check": "eslint ./src ./test",
    "typecheck": "tsc --noEmit",
    "docs:update": "cp build/quis.min.js docs/",
    "all": "npm run clean && npm run lint && npm run typecheck && npm run compile && npm run build && npm run test:all"
  },
  "author": "Dan Cox",
  "license": "MIT",
  "dependencies": {
    "peggy": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.24.0",
    "@eslint/js": "^9.36.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "babel-jest": "^30.2.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^6.8.1",
    "eslint": "^9.36.0",
    "eslint-plugin-jest": "^29.0.1",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "type": "module"
}
